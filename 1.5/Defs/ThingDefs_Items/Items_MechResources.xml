<?xml version="1.0" encoding="UTF-8"?>  


<Defs>

    <!-- Talus Synth Head-->

    <ThingDef ParentName= "ResourceBase">
    <defName>pphhyy_TalusSynthHead</defName>
    <label>talus synth head</label>
    <description>PLACEHOLDER</description>
    <possessionCount>1</possessionCount>
    <stackLimit>10</stackLimit>
    <graphicData>
      <texPath>Resources/TalusSynthHead</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
        <statBases>
          <Mass>1</Mass>
          <WorkToMake>100</WorkToMake>
        </statBases>
    <costList>
      <Steel>40</Steel>
      <Plasteel>40</Plasteel>
      <ComponentSpacer>1</ComponentSpacer>
      <SubcoreRegular>1</SubcoreRegular>
    </costList>
    <techLevel>Spacer</techLevel>
    <recipeMaker>
      <researchPrerequisite>pphhyy_URMCPearlTechnicaTier1</researchPrerequisite>
      <displayPriority>110</displayPriority>
        <recipeUsers>
            <li>FabricationBench</li>
        </recipeUsers>
    </recipeMaker>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>

